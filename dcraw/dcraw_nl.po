msgstr "Afbeelding oprekken...\n"

msgid "\nRaw photo decoder \"dcraw\" v%s"
msgstr "\ndecoder voor RAW-afbeeldingen \"dcraw\" v%s"

msgid "\nby Dave Coffin, dcoffin a cybercom o net\n"
msgstr "\ndoor Dave Coffin, dcoffin a cybercom o net\n"

msgid "\nUsage:  %s [OPTION]... [FILE]...\n\n"
msgstr "\nGebruik:  %s [OPTIE]... [BESTAND]...\n\n"

msgid "-v        Print verbose messages"
msgstr "-v        Meldingen afdrukken"

msgid "-c        Write image data to standard output"
msgstr "-c        Schrijf afbeelding naar standaarduitvoer"

msgid "-e        Extract embedded thumbnail image"
msgstr "-e        Extraheer ingebedde miniatuurafbeelding"

msgid "-i        Identify files without decoding them"
msgstr "-i        Toon korte omschrijving van bestanden, zonder ze te decoderen"

msgid "-i -v     Identify files and show metadata"
msgstr "-i -v     Toon uitgebreide omschrijving van bestanden"

msgid "-z        Change file dates to camera timestamp"
msgstr "-z        Verander bestandstijden naar datum van opname"

msgid "-a        Use automatic white balance"
msgstr "-a        Gebruik automatische witbalans"

msgid "-w        Use camera white balance, if possible"
msgstr "-w        Gebruik witbalans van de camera, indien mogelijk"

msgid "-a        Average the whole image for white balance"
msgstr "-a        Middel de gehele afbeelding voor de witbalans"

msgid "-A <x y w h> Average a grey box for white balance"
msgstr "-A <x y b h> Middel een grijs vlak voor de witbalans"

msgid "-r <r g b g> Set custom white balance"
msgstr "-r <r g b g> Gebruik handmatige witbalans"

msgid "-r <4 numbers> Set custom white balance"
msgstr "-r <4 nummers> Gebruik handmatige witbalans"

msgid "+M/-M     Use/don't use an embedded color matrix"
msgstr "+M/-M     Gebruik wel/geen ingebedde kleurenmatrix"

msgid "-C <r b>  Correct chromatic aberration"
msgstr "-C <r b>  Corrigeer chromatische aberratie"

msgid "-P <file> Fix the dead pixels listed in this file"
msgstr "-P <file> Repareer de in dit bestand opgenomen dode pixels"

msgid "-K <file> Subtract dark frame (16-bit raw PGM)"
msgstr "-K <file> Donkerbeeld aftrekken (16-bits ruwe PGM)"

msgid "-k <num>  Set the darkness level"
msgstr "-k <num>  Stel het zwartniveau in"

msgid "-S <num>  Set the saturation level"
msgstr "-S <num>  Stel het witniveau in"

msgid "-n <num>  Set threshold for wavelet denoising"
msgstr "-n <num>  Stel de grenswaarde in voor de wavelet ruisonderdrukking"

msgid "-k <num>  Set black point"
msgstr "-k <num>  Stel het zwartpunt in"

msgid "-H [0-9]  Highlight mode (0=clip, 1=no clip, 2+=recover)"
msgstr "-H [0-9]  Hoge lichten instelling (0=clip, 1=geen clip, 2+=herstellen)"

msgid "-H [0-9]  Highlight mode (0=clip, 1=unclip, 2=blend, 3+=rebuild)"
msgstr "-H [0-9]  Hoge lichten instelling (0=clip, 1=geen clip, 2=mengen, 3+=herstellen)"

msgid "-t [0-7]  Flip image (0=none, 3=180, 5=90CCW, 6=90CW)"
msgstr "-t [0-7]  Draai afbeelding (0=niet, 3=180, 5=90klok-tegen, 6=90klok-mee)"

msgid "-o [0-5]  Output colorspace (raw,sRGB,Adobe,Wide,ProPhoto,XYZ)"
msgstr "-o [0-5]  Uitvoer kleurruimte (RAW,sRGB,Adobe,Wide,ProPhoto,XYZ)"

msgid "-o <file> Apply output ICC profile from file"
msgstr "-o <file> Pas ICC-profiel van bestand toe op uitvoer"

msgid "-p <file> Apply camera ICC profile from file or \"embed\""
msgstr "-p <file> Pas camera ICC-profiel toe van bestand of \"embed\""

msgid "-d        Document mode (no color, no interpolation)"
msgstr "-d        Documentmodus (geen kleur, geen interpolatie)"

msgid "-D        Document mode without scaling (totally raw)"
msgstr "-D        Documentmodus zonder schaling (helemaal ruw)"

msgid "-j        Don't stretch or rotate raw pixels"
msgstr "-j        Ruwe pixels niet oprekken of draaien"

msgid "-W        Don't automatically brighten the image"
msgstr "-W        Afbeelding niet automatisch lichter maken."

msgid "-b <num>  Adjust brightness (default = 1.0)"
msgstr "-b <num>  Pas helderheid aan (standaard = 1.0)"

msgid "-g <p ts> Set custom gamma curve (default = 2.222 4.5)"
msgstr "-g <p ts> Stel handmatig de gamma curve in (default = 2.222 4.5)"

msgid "-q [0-3]  Set the interpolation quality"
msgstr "-q [0-3]  Stel de kwaliteit van de interpolatie in"

msgid "-h        Half-size color image (twice as fast as \"-q 0\")"
msgstr "-h        Kleurenafbeelding op halve grootte (tweemaal zo snel als \"-q 0\")"

msgid "-f        Interpolate RGGB as four colors"
msgstr "-f        Interpoleer RGGB als vier kleuren"

msgid "-m <num>  Apply a 3x3 median filter to R-G and B-G"
msgstr "-m <num>  Pas een 3x3 mediaanfilter toe op R-G en B-G"

msgid "-s [0..N-1] Select one raw image or \"all\" from each file"
msgstr "-s [0..N-1] Selecteer een bepaalde afbeelding of alle uit ieder bestand"

msgid "-6        Write 16-bit instead of 8-bit"
msgstr "-6        Schrijf 16-bits i.p.v. 8-bits gegevens"

msgid "-4        Linear 16-bit, same as \"-6 -W -g 1 1\""
msgstr "-4        Lineair 16-bits, zelfde als \"-6 -W -g 1 1\""

msgid "-B <domain> <range> Apply bilateral filter to smooth noise"
msgstr "-B <domein> <bereik> Pas bilateraal filter toe om ruis te vereffenen"

msgid "-s [0-99] Select a different raw image from the same file"
msgstr "-s [0-99] Selecteer een andere RAW-afbeelding uit hetzelfde bestand"

msgid "-4        Write 16-bit linear instead of 8-bit with gamma"
msgstr "-4        Schrijf 16-bits lineair in plaats van 8-bits met gamma"

msgid "-T        Write TIFF instead of PPM"
msgstr "-T        Schrijf TIFF in plaats van PPM"

