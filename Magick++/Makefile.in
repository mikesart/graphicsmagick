@WITH_PERL_TRUE@all-local: ../www/Magick++/NEWS.html ../www/Magick++/ChangeLog.html

# Build HTML version of news
@WITH_PERL_TRUE@../www/Magick++/NEWS.html: NEWS
@WITH_PERL_TRUE@	@PERL@ $(top_srcdir)/scripts/txt2html -t 'Magick++ News' < $(srcdir)/NEWS  > $(srcdir)/../www/Magick++/NEWS.html
@WITH_PERL_TRUE@	echo "Please do a 'cvs commit www/Magick++/NEWS.html' to submit updated HTML file"

# Build HTML version of ChangeLog
@WITH_PERL_TRUE@../www/Magick++/ChangeLog.html: ChangeLog
@WITH_PERL_TRUE@	@PERL@ $(top_srcdir)/scripts/txt2html -t 'Magick++ ChangeLog' < $(srcdir)/ChangeLog > $(srcdir)/../www/Magick++/ChangeLog.html
@WITH_PERL_TRUE@	echo "Please do a 'cvs commit www/Magick++/ChangeLog.html' to submit updated HTML file"

@WITH_PERL_FALSE@all-local:
# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
